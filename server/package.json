{
  "name": "candilib",
  "version": "2.0.0-alpha.0",
  "description": "Réserver sa place d'examen pratique du permis de conduire",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run test",
    "build": "babel src -d dist --copy-files",
    "db": "docker-compose -f docker-compose.dev.db.yml up -d",
    "db:down": "docker-compose -f docker-compose.dev.db.yml down",
    "build:all": "docker-compose -f docker-compose.prod.all.yml build",
    "dev-setup": "node dev-setup/index.js",
    "dev": "nodemon --inspect --watch src boot-dev.js",
    "dev_pm2": "pm2-runtime start ecosystem.config.js --watch --env develop",
    "format": "eslint src --fix",
    "lint:all": "run-p lint:client lint --",
    "lint:client": "cd .. ; cd client ; npm run lint",
    "lint:only": "eslint src",
    "prelint": "npm run format",
    "lint": "eslint src",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:only": "cross-env NODE_ENV=test jest",
    "pretest": "npm run lint",
    "test:all": "run-s test test:client",
    "test:client": "cd .. ; cd client ; npm test",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "jsdoc:watch": "nodemon --watch src --watch README.md --exec 'npm run jsdoc'",
    "jsdoc:serve": "browser-sync start --no-ui --server --open --files out/**/* --ss out",
    "jsdoc-dev": "run-p jsdoc:*",
    "jsdoc": "jsdoc -c jsdoc-config.js -r src README.md",
    "pm2Monitor": "pm2 monit",
    "pm2ls": "pm2 list"
  },
  "keywords": [
    "Candidat",
    "libre",
    "examen",
    "pratique",
    "permis",
    "conduire"
  ],
  "author": "Ministère de l'intérieur",
  "license": "ISC",

  "dependencies": {
    "agenda": "^4.2.1",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "delay": "^4.4.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "fast-csv": "^4.3.6",
    "full-icu": "^1.3.4",
    "generate-password": "^1.6.0",
    "jimp": "^0.16.1",
    "joi": "^17.4.1",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^9.0.0",
    "latinize": "^0.4.1",
    "lodash": "^4.17.21",
    "luxon": "^1.28.0",
    "mime": "^2.5.2",
    "mongoose": "^5.13.15",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.2",
    "nodemailer-html-to-text": "^3.2.0",
    "pm2": "^5.2.0",
    "sanitize-html": "^2.4.0",
    "stream-buffers": "^3.0.2",
    "svg-captcha-express": "^1.0.8",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "xlsx-stream-reader": "^1.1.1"
  },
  "overrides":{
    "@jimp/jpeg": {
      "jpeg-js": "^0.4.4"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@types/express": "^4.17.13",
    "@types/joi": "^14.3.4",
    "agent-base": "^6.0.2",
    "babel-jest": "^27.2.5",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "browser-sync": "^2.27.9",
    "dotenv": "^8.6.0",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "faker": "^5.5.3",
    "html-loader": "^1.3.2",
    "husky": "^4.3.8",
    "jest": "^27.2.5",
    "jsdoc": "^3.6.10",
    "jsdoc-plugin-typescript": "^2.0.6",
    "lint-staged": "^11.2.3",
    "mailparser": "^3.4.0",
    "mongodb-memory-server": "^8.0.4",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.7",
    "smtp-server": "^3.9.0",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^4.3.2",
    "swagger-ui-express": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:all"
    }
  },
  "lint-staged": {
    "../**/*.{js,vue,htm,html,css,scss}": [
      "npm run lint:all",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ]
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "plugin:import/errors",
      "plugin:import/warnings",
      "standard"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "import/no-unresolved": [
        2,
        {
          "commonjs": true,
          "amd": true
        }
      ],
      "import/named": 2,
      "import/namespace": 2,
      "import/default": 2,
      "import/export": 2,
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "config": {
    "mongodbMemoryServer": {
      "disablePostinstall": false,
      "runtimeDownload": true,
      "version": "4.0.6",
      "downloadDir": "./node_modules/.cache/mongodb-binaries"
    }
  }
}
