{
  "name": "back-office",
  "version": "2.0.0-alpha.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" vue-cli-service test:e2e",
    "lint": "run-p lint:*",
    "build:admin": "vue-cli-service build",
    "build:candidat": "vue-cli-service build",
    "cypress": "cypress run",
    "format": "run-s lint:js",
    "lint:js": "vue-cli-service lint",
    "lint:style": "stylelint src/**/*.{vue,css,sss,less,scss,sass,styl}",
    "prebuild:admin": "cp .env.admin .env",
    "prebuild:candidat": "cp .env.candidat .env",
    "sb": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "start": "npm run serve",
    "test": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" npm run -s test:unit",
    "test:e2e-local": "npm run test:e2e -- --env build=1 --config numTestsKeptInMemory=3",
    "test:e2e-local-headless": "npm run test:e2e -- --env build=1 --config numTestsKeptInMemory=3 --headless",
    "test:watch": "NODE_ICU_DATA=\"$(pwd)/node_modules/full-icu\" npm run -s test:unit -- --watch"
  },
  "dependencies": {
    "ag-grid-community": "^23.2.1",
    "ag-grid-vue": "^23.2.1",
    "chart.js": "^2.9.3",
    "core-js": "^3.8.2",
    "cronstrue": "^1.122.0",
    "delay": "^4.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "exceljs": "^4.2.0",
    "html-loader": "^1.3.0",
    "intl": "^1.2.5",
    "lodash-es": "^4.17.20",
    "luxon": "^1.25.0",
    "normalize.css": "^8.0.1",
    "p-debounce": "^2.1.0",
    "register-service-worker": "^1.7.2",
    "uuid": "^8.3.0",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.12",
    "vue-chartjs": "^3.5.1",
    "vue-class-component": "^7.2.5",
    "vue-drag-resize": "^1.4.2",
    "vue-intl": "^3.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vue-scrollto": "^2.18.2",
    "vuetify": "^2.4.10",
    "vuex": "^3.6.0",
    "whatwg-fetch": "^3.5.0"
  },
  "optionalDependencies": {
    "cypress": "^9.5.0",
    "cypress-file-upload": "^5.0.8",
    "@vue/cli-plugin-e2e-cypress": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/eslint-parser": "^7.17.0",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-pwa": "^5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-plugin-vuex": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.2",
    "@vue/vue2-jest": "^v27.0.0-alpha.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.5.0",
    "full-icu": "^1.4.0",
    "jest": "^27.1.0",
    "lint-staged": "^12.3.4",
    "material-design-icons-iconfont": "^6.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-current-selector": "0.0.3",
    "postcss-nested": "^5.0.6",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-nesting": "^10.1.2",
    "postcss-preset-env": "^7.4.1",
    "quoted-printable": "^1.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "~1.49.8",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-plugin-stylus": "^0.13.1",
    "stylus": "^0.56.0",
    "stylus-loader": "^3.0.2",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^3.0.23",
    "typeface-roboto": "1.1.13",
    "vue-cli-plugin-tailwind": "^2.2.18",
    "vue-cli-plugin-vuetify": "^2.4.6",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "vue/no-v-html": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss/nesting": {},
      "tailwindcss": {},
      "autoprefixer": {},
      "postcss-preset-env": {},
      "postcss-nested-ancestors": {},
      "postcss-nested": {},
      "postcss-current-selector": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue2-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/src/**/*.spec.js|**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es)"
    ]
  }
}
